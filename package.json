{
  "name": "@ircam/sc-scheduling",
  "version": "0.1.6",
  "description": "Set of abstractions for advanced audio scheduling in Node.js and the browser.",
  "author": [
    "Benjamin Matuszewski",
    "Norbert Schnell"
  ],
  "license": "BSD-3-Clause",
  "unpkg": "./build/bundle.js",
  "exports": {
    ".": "./src/index.js",
    "./": "./src/"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ircam-ismm/sc-scheduling"
  },
  "bugs": {
    "url": "https://github.com/ircam-ismm/sc-scheduling/issues"
  },
  "homepage": "https://ircam-ismm.github.io/sc-scheduling/",
  "keywords": [
    "web audio",
    "audio",
    "scheduling"
  ],
  "scripts": {
    "api": "jsdoc-to-readme --src src/index.js",
    "build": "npx esbuild ./src/index.js --bundle --minify --format=esm --outfile=build/bundle.js",
    "doc": "npm run api && npm run toc",
    "preversion": "npm run build",
    "test": "mocha",
    "toc": "markdown-toc -i README.md --maxdepth 3"
  },
  "devDependencies": {
    "@ircam/sc-components": "^3.0.0-alpha.31",
    "@ircam/sc-gettime": "^1.0.1",
    "chai": "^4.3.7",
    "esbuild": "^0.19.8",
    "jsdoc-to-markdown": "^8.0.0",
    "jsdoc-to-readme": "^2.0.2",
    "json5": "^2.2.3",
    "lit": "^2.8.0",
    "markdown-toc": "^1.2.0",
    "mocha": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "serve": "^14.2.0",
    "showdown": "^2.1.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@ircam/sc-utils": "^1.3.0"
  }
}
